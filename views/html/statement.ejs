<!DOCTYPE html>
<html lang="en" ng-app="caknow">
  <head>
    <meta charset="utf-8">
    <title>Superbear</title>
    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/css/master.css">
    <link rel="stylesheet" href="/css/style_0506.css">
    <link rel="stylesheet" href="/css/requestdashborad.css">
    <link rel="stylesheet" href="/css/statement.css">
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <script src="/javascripts/angular.js"></script>
    <script src="/javascripts/masterController.js"></script>
  </head>
  <body ng-controller="caknowMaster">
    <nav class="navbar navbar-default navbar-static-top masterheader">
      <!-- Bootstrap Default navbar-->
      <div class="container-fluid">
        <div class="navbar-hearder">
          <!-- navbar header ocuupy 3 columns--><a class="navbar-brand" id="brandname">Superbear</a>
        </div>
        <div class="collapse navbar-collapse">
          <ul class="nav navbar-nav navlist">
            <!-- middle menu list-->
            <li ng-click="selectMain(2)"> Service Request </li>
            <li ng-click="selectMain(3)"> Automatic Quote </li>
            <li ng-click="selectMain(4)"> My Statement </li>
            <li ng-click="selectMain(5)"> Auto Parts </li>
            <li ng-click="selectMain(6)"> Profile </li>
          </ul>
          <!-- ends of middle menu list-->
          <ul class="nav navbar-nav navbar-right headerright navlist">
            <!-- right menu list-->
            <li>HELP <i class="fa fa-question-circle fa-lg"></i></li>
            <li>LOG IN <i class="fa fa-sign-in fa-lg" aria-hidden="true"></i></li>
          </ul>
          <!-- ends of right menu list-->
        </div>
      </div>
    </nav>
    <div class="contents">
      <div class="leftside col-md-2 middle row">
        <!-- left side bar menu-->
        <div class="sidetitle">
          <!-- side title-->
          <h4>{{sidetitle}}</h4>
        </div>
        <!-- side title ends-->
        <div class="serviceR" ng-if="sidemenu == 2">
          <!-- if main menu == serviceR-->
          <ul>
            <li>New Arrived</li>
            <li>Quoted</li>
            <li>Quote Accepted</li>
            <li>- Schedule</li>
            <li>- In service</li>
            <li>Payment Pending</li>
            <li>Completed</li>
            <li>Cancelation</li>
          </ul>
        </div>
        <div class="statement" ng-if= "sidemenu == 4">
            <p>My Statements</p>
        </div>
        <div class="profile" ng-if="sidemenu == 6">
          <!-- if main menu == profile-->
          <ul>
            <li>Shop Information</li>
            <li>Contact</li>
            <li>Certifications</li>
            <li>Bussiness Hours</li>
            <li>Vehicle Makes</li>
            <li>Services</li>
            <li>Warranty</li>
            <li>Rates Settings</li>
            <li>Payment</li>
            <li>Notification</li>
            <li>Change password</li>
          </ul>
        </div>
        <!-- profile ends-->
      </div>
      <!-- ends of the left side-->
      <div class="RequestDashborad col-md-10" ng-controller="requsetController">

        <div class="requestdashborad_left col-md-3">
          <h5>Filter and Sort</h5>

          <div class="list-group">

            <div class="panel panel-default">
              <div class="panel-heading">
                <h6 class="panel-title">By Year</h6>
                <div class="filterreset">Reset</div>
              </div>
              <div class="panel-body">
                <input type="number" min="1980" max="2017" value="2016" ng-model="filterByyear[0]">
                To
                <input type="number" min="1980" max="2017" value="2017" ng-model="filterByyear[1]">
              </div>
            </div>
            
            <div class="panel panel-default">
              <div class="panel-heading">
                <h6 class="panel-title">By Make</h6>
                <div class="filterreset">Reset</div>
              </div>
              <div class="panel-body">
                <select class="carMake" >
                  <option value="Alfa Romeo">Alfa Romeo</option>  
                  <option value="Aston Martin">Aston Martin</option> 
                  <option value="Audi">Audi</option> 
                  <option value="Bentley">Bentley</option> 
                  <option value="Benz">Benz</option>  
                  <option value="BMW">BMW</option>  
                  <option value="Bugatti">Bugatti</option>   
                  <option value="Cadillac">Cadillac</option> 
                  <option value="Chrysler">Chrysler</option>  
                  <option value="Citroën">Citroën</option>  
                  <option value="Corvette">Corvette</option>  
                  <option value="DAF">DAF</option>  
                  <option value="Dacia">Dacia</option> 
                  <option value="Daewoo">Daewoo</option> 
                  <option value="Daihatsu">Daihatsu</option>  
                  <option value="Datsun">Datsun</option>  
                  <option value="De Lorean">De Lorean</option>  
                  <option value="Dino">Dino</option>  
                  <option value="Dodge">Dodge</option>   
                  <option value="Farboud">Farboud</option>  
                  <option value="Ferrari">Ferrari</option>  
                  <option value="Fiat">Fiat</option>  
                  <option value="Ford">Ford</option> 
                  <option value="Honda">Honda</option>  
                  <option value="Hummer">Hummer</option>  
                  <option value="Hyundai">Hyundai</option> 
                  <option value="Jaguar">Jaguar</option>  
                  <option value="Jeep">Jeep</option>   
                  <option value="KIA">KIA</option>  
                  <option value="Koenigsegg">Koenigsegg</option>   
                  <option value="Lada">Lada</option> 
                  <option value="Lamborghini">Lamborghini</option> 
                  <option value="Lancia">Lancia</option>  
                  <option value="Land Rover">Land Rover</option>  
                  <option value="Lexus">Lexus</option> 
                  <option value="Ligier">Ligier</option>  
                  <option value="Lincoln">Lincoln</option>  
                  <option value="Lotus">Lotus</option>   
                  <option value="Martini">Martini</option>  
                  <option value="Maserati">Maserati</option>  
                  <option value="Maybach">Maybach</option>  
                  <option value="Mazda">Mazda</option>  
                  <option value="McLaren">McLaren</option>  
                  <option value="Mercedes-Benz">Mercedes-Benz</option>  
                  <option value="Mini">Mini</option>  
                  <option value="Mitsubishi">Mitsubishi</option>   
                  <option value="Nissan">Nissan</option>  
                  <option value="Noble">Noble</option>  
                  <option value="Opel">Opel</option>   
                  <option value="Peugeot">Peugeot</option>  
                  <option value="Pontiac">Pontiac</option> 
                  <option value="Porsche">Porsche</option>   
                  <option value="Renault">Renault</option> 
                  <option value="Rolls-Royce">Rolls-Royce</option>  
                  <option value="Saab">Saab</option>  
                  <option value="Seat">Seat</option> 
                  <option value="Škoda">Škoda</option>  
                  <option value="Smart">Smart</option>  
                  <option value="Spyker">Spyker</option>   
                  <option value="Subaru">Subaru</option> 
                  <option value="Suzuki">Suzuki</option>  
                  <option value="Toyota">Toyota</option>   
                  <option value="Vauxhall">Vauxhall</option>  
                  <option value="Volkswagen">Volkswagen</option>  
                  <option value="Volvo">Volvo</option> 
                </select>
              </div>
            </div>

            <div class="panel panel-default">
              <div class="panel-heading">
                <h6 class="panel-title">By Catalog</h6>
                <div class="filterreset">Reset</div>
              </div>
              <div class="panel-body">
                <select class="carMake">
                 <option>Brake</option>
                 <option>Engine</option>
                 <option>A/C system</option>
                </select>
              </div>
            </div>

            <div class="panel panel-default">
              <div class="panel-heading">
                <h6 class="panel-title">Sort</h6>
                <div class="filterreset">Reset</div>
              </div>
              <div class="panel-body">
              <select class="carMake">
                 <option>Distance (Near to Far)</option>
                 <option>Distance (Far to Near)</option>
                </select>
              <select class="carMake">
                 <option>Quote Number(More to Less)</option>
                 <option>Quote Number(Less to More)</option>
                </select>
                <select class="carMake">
                 <option>Time(Near to Far)</option>
                 <option>Time(Far to Near)</option>
                </select>
                <select class="carMake">
                 <option>Description(Yes)</option>
                 <option>Description(No)</option>
                </select>
              </div>
            </div>

          </div>

        </div>
        <div class="requestdashborad_right col-md-9">
        </div>
      </div>
    <div class="footer">
      <!-- footer-->
    </div>
    <!-- ends of the footer-->
  </body>
</html>